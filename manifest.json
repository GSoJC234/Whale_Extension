{
  "manifest_version": 2,
  "name": "Travelers",
  "version" : "1.0.0",

  "description" : "Helping oraganize transport plan in traveling",
  "default_locale" : "ko",

  "permissions": [
  "storage", "tabs"
  ],

  "content_security_policy": "script-src 'self' https://developers.kakao.com/sdk/js/kakao.min.js https://code.jquery.com/jquery-3.2.1.min.js https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js ; object-src 'self'",
  "sidebar_action" : {
    "default_page": "static/index.html",
    "default_icon" : {
      "16": "images/icon16.png"
    },
    "default_title" : "SidebarApp",
    "use_navigation_bar" : false
  }

}
